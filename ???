local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Window = Rayfield:CreateWindow({
   Name = "⏳ PNB Limited Bypass ⏳",
   Icon = 0, 
   LoadingTitle = "Limited Bypass",
   LoadingSubtitle = "by ???",
   ShowText = "", 
   Theme = "Default",

   ToggleUIKeybind = "K",

   DisableRayfieldPrompts = false,
   DisableBuildWarnings = false,

   ConfigurationSaving = {
      Enabled = false,
      FolderName = nil,
      FileName = ""
   },

   KeySystem = true, 
   KeySettings = {
      Title = "PNB Limited Bypass",
      Subtitle = "Key System",
      Note = "Ask the owner for key",
      FileName = "Limited Bypasser Key",
      SaveKey = true,
      GrabKeyFromSite = false,
      Key = {"owner", "ayonxd3481"}
   }
})


local MainTab = Window:CreateTab("Main", nil) -- Title, Image

--update 1--
local MainSection = MainTab:CreateSection("Limited Update 1")

local Button = MainTab:CreateButton({
   Name = "Click 150 Tuned | 300.000.000",
   Callback = function()
   local args = {
	"[Limited] Click 150 Tuned"
}
game:GetService("ReplicatedStorage"):WaitForChild("DealershipEvents"):WaitForChild("BuyCar"):InvokeServer(unpack(args))
   end,
})

